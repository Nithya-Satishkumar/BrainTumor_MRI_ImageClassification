ge # For image processing\n","import os # For path operations\n","\n","## Model Loading - Cached for Efficiency ##\n","@st.cache_resource # Caches the model so it's loaded only once\n","def load_mobilenet_model():\n","    ## Loading the pretrained MobileNet model from Google Drive ##\n","    model_path = '/content/drive/MyDrive/MobileNetModel.h5'\n","\n","    if not os.path.exists(model_path):\n","        st.error(f\"Model file not found at: {model_path}\")\n","        st.stop() # Stop the Streamlit app if the model file isn't found\n","\n","    try:\n","        ## Loading the entire model (architecture, weights, optimizer state) ##\n","        model = tf.keras.models.load_model(model_path)\n","        st.success(\"Model loaded successfully from .h5 file!\")\n","    except Exception as e:\n","        st.error(f\"Could not load model from {model_path}: {e}\")\n","        st.stop() # Stop the Streamlit app if model can't be loaded\n","\n","    return model\n","\n","## Loading the model ##\n","model = load_mobilenet_model()\n","\n","## Defining Class Names . Order must be same as training ##\n","class_names = ['glioma', 'meningioma', 'no_tumor', 'pituitary']\n","\n","## Streamlit UI ##\n","#st.set_page_config(layout=\"wide\")\n","st.title('ğŸ§  Brain Tumor Classification ')\n","\n","st.write(\"Upload an MRI image to get a prediction for brain tumor type\")\n","\n","uploaded_file = st.file_uploader(\"Choose an image...\", type=[\"jpg\", \"jpeg\", \"png\"])\n","\n","if uploaded_file is not None:\n","    ## Displaying the uploaded image ##\n","    st.image(uploaded_file, caption='Uploaded Image.', width=200)\n","    st.write(\"\")\n","    st.write(\"Classifying...\")\n","\n","    ## Preprocessing the image - Same as training ##\n","    img = Image.open(uploaded_file).convert('RGB') # Ensure 3 channels\n","    img = img.resize((224, 224)) # Resize to model's input shape\n","    img_array = image.img_to_array(img) # Convert to numpy array\n","    img_array = np.expand_dims(img_array, axis=0) # Add batch dimension (1, 224, 224, 3)\n","    img_array = img_array / 255.0 # Normalize pixel values (0-1)\n","\n","    ## Make prediction ##\n","    with st.spinner('Predicting...'):\n","        predictions = model.predict(img_array)\n","        predicted_class_index = np.argmax(predictions[0])\n","        predicted_class_name = class_names[predicted_class_index]\n","        confidence = predictions[0][predicted_class_index] * 100\n","\n","    st.success(f\"Prediction: **{predicted_class_name.replace('_', ' ').title()}**\")\n","    st.info(f\"Confidence: **{confidence:.2f}%**\")\n","\n","    st.markdown(\"---\")\n","    st.subheader(\"All Probabilities:\")\n","    ## Displaying all class probabilities ##\n","    import pandas as pd\n","    prob_df = pd.DataFrame({\n","        'Tumor Type': [name.replace('_', ' ').title() for name in class_names],\n","        'Probability (%)': [p * 100 for p in predictions[0]]\n","    })\n","    st.dataframe(prob_df.sort_values(by='Probability (%)', ascending=False).reset_index(drop=True))\n","\n","st.markdown(\"---\")"]},{"cell_type":"code","execution_count":12,"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"executionInfo":{"elapsed":125,"status":"ok","timestamp":1754648686539,"user":{"displayName":"Nithya S","userId":"01423115534669098575"},"user_tz":-330},"id":"89mZwvixHC1y","outputId":"ca2aa3bb-57e3-4cba-d350-89a15fc1cbc5"},"outputs":[{"output_type":"stream","name":"stdout","text":["35.199.32.64\n"]}],"source":["## To get the public IPv4 address ##\n","!wget -q -O - ipv4.icanhazip.com"]},{"cell_type":"code","execution_count":14,"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"jee2FEb-FQgy","executionInfo":{"status":"ok","timestamp":1754648807158,"user_tz":-330,"elapsed":76795,"user":{"displayName":"Nithya S","userId":"01423115534669098575"}},"outputId":"39c0325a-5ed3-4d2a-c4c4-e0dcbd822248"},"outputs":[{"output_type":"stream","name":"stdout","text":["\n","Collecting usage statisti